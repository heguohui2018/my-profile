{
	"coc.preferences.enableFloatHighlight":true,
	"coc.preferences.colorSupport": true,
	"coc.preferences.hoverTarget": "float",
	"coc.preferences.messageLevel": "more",
	"coc.preferences.jumpCommand": "edit",
    "suggest.noselect": false,
    "suggest.triggerAfterInsertEnter": true,
	"suggest.snippetIndicator": " $ ",
	"suggest.floatEnable": true,
    "snippets.snipmate.enable": true,
    "json.enable": true,
    "html.enable": true,
	"html.format.enable":true,
    "html.filetypes": ["html","handlebars","php"],
	"html.suggest.html5":true,
	"html.format.indentInnerHtml":true,
	"html.validate.html":true,
    "html.validate.scripts": true,
    "html.validate.styles": true,
    "css.enable": true,
	"css.colorDecorators.enable":true,
    "css.validate": false,
	"scss.validate": false,
	"stylelint.enable": true,
    "translator.engines": ["google", "bing"],
	"translator.window.maxWidth":300,
	"translator.window.maxHeight":400,
	"javascript.suggest.names": true,
	"javascript.showUnused": true,
	"javascript.validate.enable": true,
	"javascript.preferences.importModuleSpecifier": "relative",	
	"phpls.enable":true,
	"tsserver.enable": true,
	"vetur.completion.autoImport": true,
	"vetur.format.options.tabSize": 4,
	"highlight.document.enable": true,
	"highlight.colors.enable":true,
	"git.enableGlobalStatus": true,
	"prettier.useTabs": true,
	"prettier.tabWidth":4,
	"explorer.width": 35,
	"explorer.icon.enableNerdfont": true,
	"explorer.icon.enableVimDevions": true,
	"explorer.keyMappings": {
		"k": "nodePrev",
		"j": "nodeNext",

		"*": "toggleSelection",
		"<tab>": "actionMenu",

		"h": "collapse",
		"l": "expand",
		"J": ["toggleSelection", "normal:j"],
		"K": ["toggleSelection", "normal:k"],
		"gl": "expandRecursive",
		"gh": "collapseRecursive",
		"o": "expandOrCollapse",
		"<cr>": "open",
		"e": "open",
		"E": "openInVsplit",
		"t": "openInTab",
		"<bs>": "gotoParent",

		"y": "copyFilepath",
		"Y": "copyFilename",
		"c": "copyFile",
		"x": "cutFile",
		"p": "pasteFile",
		"d": "delete",
		"D": "deleteForever",

		"a": "addFile",
		"A": "addDirectory",
		"r": "rename",

		".": "toggleHidden",
		"R": "refresh",

		"?": "help",
		"q": "quit",
		"X": "systemExecute",
		"gd": "listDrive",

		"f": "search",
		"F": "searchRecursive",

		"gf": "gotoSource:file",
		"gb": "gotoSource:buffer",

		"[[": "sourcePrev",
		"]]": "sourceNext",

		"[d": "diagnosticPrev",
		"]d": "diagnosticNext",

		"[c": "gitPrev",
		"]c": "gitNext",
		"<<": "gitStage",
		">>": "gitUnstage"
	}
}
